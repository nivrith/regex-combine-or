{
  "name": "regex-combine-or",
  "version": "1.0.1",
  "description": "Combines an array of regexes into a single regex using `or` operator `|` ",
  "scripts": {
    "compile": "npm run clean && tsc -b",
    "start": "yarn compile && node dist/src/index",
    "test": "nyc mocha --require ts-node/register test/**/*.spec.ts",
    "prepublishOnly": "npm run compile",
    "pretest": "npm run compile",
    "clean": "del-cli dist",
    "docs": "typedoc src"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "lines": 80,
    "exclude": [
      "test",
      "docs",
      "dist"
    ],
    "check-coverage": true
  },
  "main": "dist/src",
  "repository": "https://github.com/nivrith/regex-combine-or",
  "author": "nivrithgomatam@gmail.com",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "del-cli": "^2.0.0",
    "mocha": "^6.1.4",
    "np": "^5.0.3",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}
